<mvc:View xmlns="sap.m" xmlns:semantic="sap.f.semantic" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" controllerName="ns.EBilliaApp.controller.revokeAsn.DetailRevokeAsn">
	<semantic:SemanticPage id="page" busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:titleHeading>
			<Title text="{      parts: [       'i18n>ASNListRA',       'asnDetailView&gt;/ASN'      ],      formatter: 'jQuery.sap.formatMessage'     }"/>
		</semantic:titleHeading>
		<semantic:content>
			<l:VerticalLayout>
			
				<Table id="lineItemsList" class="sapUiSmallMarginTop" width="auto" items="{asnDetailView>/ASNDetalle}" updateFinished=".onListUpdateFinished" noDataText="{i18n>noDataTableREP}" busyIndicatorDelay="{detailView>/lineItemTableDelay}">
					<headerToolbar>
						<Toolbar id="lineItemsToolbar">
							<Title id="lineItemsHeader" text="{detailView>/lineItemListTitle}"/>
						</Toolbar>
					</headerToolbar>
					<columns>
						<Column width="5em">
							<Text text="{i18n>posicionTableREP}"/>
						</Column>
						<Column minScreenWidth="Tablet" width="12em" demandPopin="true" hAlign="Begin">
							<Text text="{i18n>materialTableREP}"/>
						</Column>
						
						<Column minScreenWidth="Tablet" width="4em" demandPopin="true" hAlign="Begin">
							<Text text="{i18n>vamTableREP}"/>
						</Column>
                        <Column width="6em" hAlign="Begin">
							<Text text="{i18n>FechaEntregaRA}"/>
						</Column>
                        <Column width="5em" hAlign="Begin">
							<Text text="{i18n>CantidadRecibidaRA}"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<ObjectIdentifier text="{asnDetailView>posicion}"/>
                               
                                <ObjectIdentifier title="{asnDetailView>material}" text="{asnDetailView>descripcion}" class="sapUiTinyMarginTopBottom"/>


                                <ObjectIdentifier text="{asnDetailView>vam}"/>
                            
								
                                <ObjectIdentifier text="{           path: 'asnDetailView>fecha_entrega'                     }"/>

								<ObjectIdentifier text="{                                         path:'asnDetailView>cantidadRecibida'                            }"/>

                                

                                
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</l:VerticalLayout>
		</semantic:content>
		
	<semantic:headerContent>
	    <l:VerticalLayout width="100%" id="layout4">
	    </l:VerticalLayout>
	</semantic:headerContent>
	<semantic:titleMainAction>
	    <semantic:TitleMainAction text="{i18n>AnularASN}" id="actionRevoke" press=".anularASN"/>
	</semantic:titleMainAction>
	</semantic:SemanticPage>
</mvc:View>