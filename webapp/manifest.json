{
    "_version": "1.12.0",
    "sap.app": {
        "id": "ns.EBilliaApp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        }
    }, 
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "ns.EBilliaApp.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.60.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ns.EBilliaApp.i18n.i18n",
                    "bundleUrl": "i18n/i18n.properties",
                    "async": true,
                    "fallbackLocale": "es",
                    "supportedLocales": [
                        "es",
                        "en"
                    ]
                }
            },
            "side": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/sideContent.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "ns.EBilliaApp.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": [
                        "TargetApp"
                    ]
                },
                {
                    "name": "login",
                    "pattern": "login",
                    "target": [
                        "TargetLogin"
                    ]
                },
                {
                    "name": "main",
                    "pattern": "",
                    "target": [
                        "TargetMain"
                    ],
                    "controlId": "app"
                },
                {
                    "name": "ConfirmacionOrdenCompra",
                    "pattern": "ConfirmacionOrdenCompra",
                    "target": [
                        "masterPurchaseOrders",
                        "detailPurchaseOrders"
                    ]
                },
                {
                    "pattern": "ConfirmacionOrdenCompra/{objectId}",
                    "name": "ConfirmacionOrdenCompraDetail",
                    "target": [
                        "masterPurchaseOrders",
                        "detailPurchaseOrders"
                    ]
                },
                {
                    "name": "cargaFactura",
                    "pattern": "cargaFactura",
                    "target": [
                         "masterInvoiceUpload",
                        "detailInvoiceUpload"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "pattern": "cargaFacturaDetail/{orderId}",
                    "name": "cargaFacturaDetail",
                    "target": [
                        "masterInvoiceUpload",
                        "detailInvoiceUpload"
                    ]
                },
                {
                    "name": "CargarComplementos",
                    "pattern": "CargarComplementos/{param}",
                    "target": [
                         "masterComplementUpload",
                        "detailComplementUpload"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "pattern": "cargarComplementosDetail/{item}/{isSelected}",
                    "name": "cargarComplementosDetail",
                    "target": [
                        "masterComplementUpload",
                        "detailComplementUpload"
                    ]
                },
                {
                    "name": "VerComplementos",
                    "pattern": "VerComplementos/{param}",
                    "target": [
                         "masterComplementSee",
                        "detailComplementSee"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "pattern": "VerComplementosDetail/{item}",
                    "name": "VerComplementosDetail",
                    "target": [
                        "masterComplementSee",
                        "detailComplementSee"
                    ]
                },
                {
                    "name": "settings",
                    "pattern": "systemSettings",
                    "target": [
                        "settings"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "name": "ReporteEntregasPendientes",
                    "pattern": "ReporteEntregasPendientes",
                    "target": ["reporteEntregasPendientes"]
                },
                {
                    "name": "NotificacionEntregaNacional",
                    "pattern": "NotificacionEntregaNacional/{param}",
                    "target": [
                        "masterDeliveryNotifications",
                        "detailDeliveryNotifications"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "pattern": "NotificacionEntregaNacionalDetail/{orderId}",
                    "name": "NotificacionEntregaNacionalDetail",
                    "target": [
                        "masterDeliveryNotifications",
                        "detailDeliveryNotifications"
                    ]
                },
                {
                    "pattern": "masterDeliveryNotificationsG1",
                    "name": "masterDeliveryNotificationsG1",
                    "target": "masterDeliveryNotificationsG1"
                },
                {
                    "name": "NotificacionEntregaInternacional",
                    "pattern": "NotificacionEntregaInternacional",
                    "target": [
                        "masterDeliveryNotificationsInt",
                        "detailDeliveryNotificationsInt"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "pattern": "NotificacionEntregaInternacional/{orderId}",
                    "name": "NotificacionEntregaInternacionalDetail",
                    "target": [
                        "masterDeliveryNotificationsInt",
                        "detailDeliveryNotificationsInt"
                    ]
                },
                {
               
                    "name": "ReporteComplemnetosPendientesCarga",
                    "pattern": "ReporteComplemnetosPendientesCarga",
                    "target": [
                        "reporteComplementos"
                    ]
                },
				{
					"pattern": "ReporteFacturas",
					"name": "ReporteFacturas",
					"target": [
						"masterInvoiceProcessed",
						"detailInvoiceProcessed"
					]
				},
				{
					"pattern": "reporteFacturaDetail/{id}",
					"name": "reporteFacturaDetail",
					"target": [
						"masterInvoiceProcessed",
						"detailInvoiceProcessed"
					]
                },
                {
					"pattern": "busquedaASN",
					"name": "busquedaASN",
					"target": [
						"masterASN",
						"detailASN"
					]
				},
				{
					"pattern": "ASNDetail/{id}",
					"name": "ASNDetail",
					"target": [
						"masterASN",
						"detailASN"
					]
                }
                
            ],
            
            "targets": {
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                },
                "TargetLogin": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "Login",
                    "viewLevel": 1
                },
                "TargetMain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "Main",
                    "viewLevel": 2
                },
                "TargetHome": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "Home",
                    "controlId": "pageContainer",
                    "controlAggregation": "pages"
                },
                "appPurchaseOrders": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layout",
                    "controlId": "pageContainer",
                    "viewName": "po.AppPurchaseOrders"
                },
                "masterPurchaseOrders": {
                    "viewName": "po.MasterPurchaseOrders",
                    "controlId": "layout",
                    "parent": "appPurchaseOrders",
                    "controlAggregation": "beginColumnPages"
                },
                "detailPurchaseOrders": {
                    "viewName": "po.DetailPurchaseOrders",
                    "controlId": "layout",
                    "controlAggregation": "midColumnPages",
                    "parent": "appPurchaseOrders"
                },
                "appInvoiceUpload": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutInvoice",
                    "controlId": "pageContainer",
                    "viewName": "invoice.AppInvoiceUpload"
                },
                "masterInvoiceUpload": {
                    "viewName": "invoice.MasterInvoiceUpload",
                    "controlId": "layoutInvoice",
                    "parent": "appInvoiceUpload",
                    "controlAggregation": "beginColumnPages"
                },
                "detailInvoiceUpload": {
                    "viewName": "invoice.DetailInvoiceUpload",
                    "controlId": "layoutInvoice",
                    "controlAggregation": "midColumnPages",
                    "parent": "appInvoiceUpload"
                },
                "complementUpload": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutComplement",
                    "controlId": "pageContainer",
                    "viewName": "complements.ComplementUpload"
                },
                "masterComplementUpload": {
                    "viewName": "complements.MasterUpload",
                    "controlId": "layoutComplement",
                    "parent": "complementUpload",
                    "controlAggregation": "beginColumnPages",
                    "viewId": "mcu"
                },
                "detailComplementUpload": {
                    "viewName": "complements.DetailUpload",
                    "controlId": "layoutComplement",
                    "controlAggregation": "midColumnPages",
                    "parent": "complementUpload"
                },
                "complementSee": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutSeeComplement",
                    "controlId": "pageContainer",
                    "viewName": "seeComplement.ComplementSee"
                },
                "masterComplementSee": {
                    "viewName": "seeComplement.MasterSee",
                    "controlId": "layoutSeeComplement",
                    "parent": "complementSee",
                    "controlAggregation": "beginColumnPages"
                },
                "detailComplementSee": {
                    "viewName": "seeComplement.DetailSee",
                    "controlId": "layoutSeeComplement",
                    "controlAggregation": "midColumnPages",
                    "parent": "complementSee"
                },
                "settings": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "SystemSettings",
                    "controlId": "pageContainer",
                    "controlAggregation": "pages"
                },
                "reporteEntregasPendientes": {
                    "viewName": "reports_.ReporteEntregasPendientes",
                    "parent": "TargetMain",
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
                    "controlId": "pageContainer",
                    "controlAggregation": "pages"
                },
                "reporteComplementos": {
                    "viewName": "reports.PendingComplement",
                    "parent": "TargetMain",
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
                    "controlId": "pageContainer",
                    "controlAggregation": "pages"
                },
                "deliveryNotifications": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutDeliveryNotifications",
                    "controlId": "pageContainer",
                    "viewName": "asn.National.DeliveryNotifications"
                },
                "masterDeliveryNotifications": {
                    "viewName": "asn.National.MasterDeliveryNotifications",
                    "controlId": "layoutDeliveryNotifications",
                    "parent": "deliveryNotifications",
                    "controlAggregation": "beginColumnPages"
                },
                "detailDeliveryNotifications": {
                    "viewName": "asn.National.DetailDeliveryNotifications",
                    "controlId": "layoutDeliveryNotifications",
                    "controlAggregation": "midColumnPages",
                    "parent": "deliveryNotifications",
                    "viewId": "ddn"
                },
                "masterDeliveryNotificationsG1": {
                    "viewName": "asn.NationalSecondView.DeliveryNotificationsSecond",
                    "parent": "TargetMain",
					"viewType": "XML",
					"transition": "slide",
                    "controlId": "pageContainer"
                },
                "deliveryNotificationsInt": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutDeliveryNotificationsInt",
                    "controlId": "pageContainer",
                    "viewName": "asn.International.DeliveryNotificationsInt"
                },
                "masterDeliveryNotificationsInt": {
                    "viewName": "asn.International.MasterDeliveryNotificationsInt",
                    "controlId": "layoutDeliveryNotificationsInt",
                    "parent": "deliveryNotificationsInt",
                    "controlAggregation": "beginColumnPages"
                },
                "detailDeliveryNotificationsInt": {
                    "viewName": "asn.International.DetailDeliveryNotificationsInt",
                    "controlId": "layoutDeliveryNotificationsInt",
                    "controlAggregation": "midColumnPages",
                    "parent": "deliveryNotificationsInt"
                },
                "appInvoicesProcessed": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutInvoiceProcessed",
                    "controlId": "pageContainer",
                    "viewName": "reports.FlexibleColumnLayout"
                },
                "masterInvoiceProcessed": {
                    "viewName": "reports.InvoicesMaster",
                    "controlId": "layoutInvoiceProcessed",
                    "parent": "appInvoicesProcessed",
                    "controlAggregation": "beginColumnPages"
                },
                "detailInvoiceProcessed": {
                    "viewName": "reports.InvoicesDetail",
                    "controlId": "layoutInvoiceProcessed",
                    "controlAggregation": "midColumnPages",
                    "parent": "appInvoicesProcessed"
                },
                "appASN": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutASN",
                    "controlId": "pageContainer",
                    "viewName": "searchAsn.AppASN"
                },
                "masterASN": {
                    "viewName": "searchAsn.MasterANS",
                    "controlId": "layoutASN",
                    "parent": "appASN",
                    "controlAggregation": "beginColumnPages"
                },
                "detailASN": {
                    "viewName": "searchAsn.DetailASN",
                    "controlId": "layoutASN",
                    "controlAggregation": "midColumnPages",
                    "parent": "appASN"
                }
                
            }
        }
    }
}