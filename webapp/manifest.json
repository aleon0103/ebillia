{
    "_version": "1.12.0",
    "sap.app": {
        "id": "ns.EBilliaApp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        }
    }, 
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "ns.EBilliaApp.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.60.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ns.EBilliaApp.i18n.i18n",
                    "bundleUrl": "i18n/i18n.properties",
                    "async": true,
                    "fallbackLocale": "es",
                    "supportedLocales": [
                        "es",
                        "en"
                    ]
                }
            },
            "side": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/sideContent.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "ns.EBilliaApp.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": [
                        "TargetApp"
                    ]
                },
                {
                    "name": "login",
                    "pattern": "login",
                    "target": [
                        "TargetLogin"
                    ]
                },
                {
                    "name": "main",
                    "pattern": "",
                    "target": [
                        "TargetMain"
                    ],
                    "controlId": "app"
                },
                {
                    "name": "ConfirmacionOrdenCompra",
                    "pattern": "ConfirmacionOrdenCompra",
                    "target": [
                        "masterPurchaseOrders",
                        "detailPurchaseOrders"
                    ]
                },
                {
                    "pattern": "ConfirmacionOrdenCompra/{objectId}",
                    "name": "ConfirmacionOrdenCompraDetail",
                    "target": [
                        "masterPurchaseOrders",
                        "detailPurchaseOrders"
                    ]
                },
                {
                    "name": "cargaFactura",
                    "pattern": "cargaFactura",
                    "target": [
                         "masterInvoiceUpload",
                        "detailInvoiceUpload"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "pattern": "cargaFacturaDetail/{orderId}",
                    "name": "cargaFacturaDetail",
                    "target": [
                        "masterInvoiceUpload",
                        "detailInvoiceUpload"
                    ]
                },
                {
                    "name": "CargarComplementos",
                    "pattern": "CargarComplementos",
                    "target": [
                         "masterComplementUpload",
                        "detailComplementUpload"
                    ],
                    "controlId": "mainContents"
                },
                {
                    "pattern": "CargarComplementosDetail",
                    "name": "CargarComplementosDetail",
                    "target": [
                        "masterComplementUpload",
                        "detailComplementUpload"
                    ]
                },
                {
                    "name": "settings",
                    "pattern": "systemSettings",
                    "target": [
                        "settings"
                    ],
                    "controlId": "mainContents"
                }
            ],
            "targets": {
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                },
                "TargetLogin": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "Login",
                    "viewLevel": 1
                },
                "TargetMain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "Main",
                    "viewLevel": 2
                },
                "TargetHome": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "Home",
                    "controlId": "pageContainer",
                    "controlAggregation": "pages"
                },
                "appPurchaseOrders": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layout",
                    "controlId": "pageContainer",
                    "viewName": "po.AppPurchaseOrders"
                },
                "masterPurchaseOrders": {
                    "viewName": "po.MasterPurchaseOrders",
                    "controlId": "layout",
                    "parent": "appPurchaseOrders",
                    "controlAggregation": "beginColumnPages"
                },
                "detailPurchaseOrders": {
                    "viewName": "po.DetailPurchaseOrders",
                    "controlId": "layout",
                    "controlAggregation": "midColumnPages",
                    "parent": "appPurchaseOrders"
                },
                "appInvoiceUpload": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutInvoice",
                    "controlId": "pageContainer",
                    "viewName": "invoice.AppInvoiceUpload"
                },
                "masterInvoiceUpload": {
                    "viewName": "invoice.MasterInvoiceUpload",
                    "controlId": "layoutInvoice",
                    "parent": "appInvoiceUpload",
                    "controlAggregation": "beginColumnPages"
                },
                "detailInvoiceUpload": {
                    "viewName": "invoice.DetailInvoiceUpload",
                    "controlId": "layoutInvoice",
                    "controlAggregation": "midColumnPages",
                    "parent": "appInvoiceUpload"
                },
                "complementUpload": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "viewId": "layoutComplement",
                    "controlId": "pageContainer",
                    "viewName": "complements.ComplementUpload"
                },
                "masterComplementUpload": {
                    "viewName": "complements.MasterUpload",
                    "controlId": "layoutComplement",
                    "parent": "complementUpload",
                    "controlAggregation": "beginColumnPages"
                },
                "detailComplementUpload": {
                    "viewName": "complements.DetailUpload",
                    "controlId": "layoutComplement",
                    "controlAggregation": "midColumnPages",
                    "parent": "complementUpload"
                },
                "settings": {
                    "parent": "TargetMain",
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "SystemSettings",
                    "controlId": "pageContainer",
                    "controlAggregation": "pages"
                }
            }
        }
    }
}